<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >
  <FrameLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      >

    <com.lyy.keepassa.widgets.MainFloatActionButton
        android:id="@+id/fabNew"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="32dp"
        android:layout_gravity="end|bottom"
        android:background="@drawable/main_fab_bg"
        android:elevation="2dp"
        app:shapeAppearanceOverlay="@style/CircleStyle"
        app:layout_behavior="com.lyy.keepassa.widgets.MainFloatActionButtonBehavior"
        tools:srcCompat="@drawable/ic_add_24px"

        />

  </FrameLayout>
</layout>
